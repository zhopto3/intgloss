\g 

\t Gɛ bɔmɔ samɛ sɛ bulĩ́ bá ɔlɔ
\b G-ɛ bɔ-mɔ s-a-mɛ sɛ bu-li-̃-́ ba-́ ɔ-lɔ
\q Gɛ bɔ-mɔ sa-mɛ sɛ bu-li-̃́ ba-́ ɔ-lɔ 
\g 

\t err bɔbakpɛ́ kesi nɛ́ dénɔ nɛ́
\b err bɔba-kpɛ́ ke-s-i nɛ́ d-é-nɔ nɛ́
\q err bɔba-kpɛ́ ke-si nɛ́ d-é-nɔ nɛ́ 
\g 

\t kɔnú kɔ́ wɔ́,
\b k-ɔ-n-ú kɔ́ w-ɔ-́,
\q kɔn-ú kɔ́ wɔ-́, 
\g 

\t ɔma shú.
\b ɔ-m-a sh-ú-.
\q ɔma sh-ú-.
\g 

\t enu srahanɛ́
\b e-nu s-r-a-h-an-ɛ́
\q e-nu srah-an-ɛ́ 
\g 

\t bɔzaabhɛtɛ yɛ,
\b bɔ-z-a-a-bhɛtɛ y-ɛ,
\q bɔ-zaa-bhɛtɛ y-ɛ, 
\g 

\t faa,
\b f-a-a,
\q fa-a, 
\g 

\t Srṍé nɔ́,
\b S-r-o-̃́-é nɔ-́,
\q Sro-̃́-é nɔ-́, 
\g 

\t oyi nɔ tɔ̃ ko
\b o-y-i nɔ t-ɔ-̃ ko
\q oyi nɔ tɔ̃ ko 
\g 

\t pɛ abavɛnɔ pɛ kɛgbãgbã babá,
\b pɛ aba-v-ɛ-nɔ pɛ kɛ-g-ba-̃-g-ba-̃ baba-́,
\q pɛ aba-vɛ-nɔ pɛ kɛ-g-ba-̃g-ba-̃ baba-́, 
\g 

\t nɛ́ kashɛ̃,
\b nɛ́ ka-sh-ɛ̃-,
\q nɛ́ ka-sh-ɛ̃-,
\g 

\t alɛ́ naálɛ
\b a-lɛ́ n-a-á-lɛ
\q a-lɛ́ na-á-lɛ 
\g 

\t bɔbabhɛtɛ ɛlɛ́,
\b bɔba-bhɛtɛ ɛ-lɛ́,
\q bɔba-bhɛtɛ ɛ-lɛ́, 
\g 

\t kpasí
\b kp-a-s-í
\q kp-as-í 
\g 

\t ɔbɔ lɔkɔ ɔɖa tolí ɔlɔkɔ pú ozi nɛ́ kɛlɛ,
\b ɔ-b-ɔ lɔkɔ ɔ-ɖ-a t-o-lí ɔ-lɔkɔ p-ú o-z-i nɛ́ kɛ-l-ɛ,
\q ɔbɔ lɔkɔ ɔɖa to-lí ɔ-lɔkɔ p-ú ozi nɛ́ kɛ-l-ɛ, 
\g 

\t ɔkɔ́ tolí ko kelĩkekpu,
\b ɔ-kɔ́ t-o-lí ko ke-li-̃-ke-kp-u-,
\q ɔ-kɔ́ to-lí ko ke-li-̃-ke-kp-u,
\g 

\t batɛ́bhɛtɛ ɛlɛ́
\b bat-ɛ́-bhɛtɛ ɛ-lɛ́
\q bat-ɛ́-bhɛtɛ ɛ-lɛ́ 
\g 

\t yoo
\b y-o-o
\q yoo
\g 

\t soɔ bunu batrugbu ɔma
\b s-o-ɔ bu-nu bat-r-u-gb-u ɔ-m-a
\q soɔ bu-nu bat-ru-gb-u ɔma
\g 

\t kɛlɔ̃́mɛ́ bɔlɔkɔ tɔ bɔwá kɔ́ anuvɔ,
\b kɛ-lɔ-̃́-m-ɛ́ bɔ-lɔkɔ t-ɔ bɔ-w-á kɔ́ an-u-v-ɔ,
\q kɛ-lɔ-̃́-m-ɛ́ bɔ-lɔkɔ tɔ bɔ-w-á kɔ́ an-uv-ɔ, 
\g 

\t bakɔ́ akakála akála,
\b ba-kɔ́ a-ka-k-á-l-a a-k-á-l-a,
\q ba-kɔ́ a-ka-k-á-la ak-á-l-a, 
\g 

\t so bulĩ́ɛ́ butsǐ buútsǐ genú
\b so bu-li-̃́-ɛ́ bu-ts-i-̌ bu-ú-ts-i-̌ g-e-n-ú
\q so bu-li-̃́-ɛ́ bu-ts-ǐ bu-ú-ts-ǐ gen-ú 
\g 

\t adzɛ̃̌ɛ́ yɛ alɔkɔ watch ɛ́, watch ɛ́
\b a-dz-ɛ̃-̌-ɛ́ yɛ a-lɔkɔ w-a-t-c-h ɛ́, w-a-t-c-h ɛ́
\q a-dz-ɛ̃-̌-ɛ́ yɛ a-lɔkɔ watch ɛ́, watch ɛ́ 
\g 

\t Klisoɔ gɛ bɔlɛ kɛ́lɛ,err nɛ́ dzíɖuɖu nɔ́ yɛ kinyé mɛ,
\b K-li-s-o-ɔ gɛ bɔ-lɛ kɛ-́-l-ɛ-,-err nɛ́ dz-í-ɖ-u-ɖ-u nɔ́ yɛ ki-ny-é mɛ,
\q K-li-soɔ gɛ bɔ-lɛ kɛ-́lɛ,-err nɛ́ dz-í-ɖuɖu nɔ́ yɛ ki-ny-é mɛ, 
\g 

\t kɛlɛ kiwi lɛ wɔ ɔvɛ odiwu
\b kɛlɛ ki-w-i lɛ w-ɔ ɔ-v-ɛ o-d-i-w-u
\q kɛlɛ ki-wi lɛ wɔ ɔvɛ odiwu
\g 

\t bebebú bushenu
\b be-be-bu-́ bu-sh-e-nu
\q be-be-bu-́ bu-sh-e-nu 
\g 

\t klɛ ɛma gɛ lɛ́ lɛ ɔbha gɛ bamɔ ana
\b klɛ ɛ-m-a gɛ lɛ́ lɛ ɔ-bh-a gɛ ba-mɔ ana
\q klɛ ɛma gɛ lɛ́ lɛ ɔ-bh-a gɛ ba-mɔ ana 
\g 

\t gɛ bɔlɛ Okúɔ́ mɛ nɛ́ ɛkɛlɛ oo,
\b gɛ bɔ-lɛ O-kú-ɔ́ mɛ nɛ́ ɛ-kɛlɛ o-o-,
\q gɛ bɔ-lɛ O-kú-ɔ́ mɛ nɛ́ ɛ-kɛlɛ oo,
\g 

\t kpɛ́ ɔɖa tólí mɛ,
\b kpɛ́ ɔ-ɖ-a t-ó-lí mɛ,
\q kpɛ́ ɔɖa t-ó-lí mɛ, 
\g 

\t kitsu pɛ bɔɔ́wa kɛlɛ́,
\b ki-ts-u pɛ bɔ-ɔ́-w-a kɛ-lɛ́,
\q ki-ts-u pɛ bɔ-ɔ́-wa kɛ-lɛ́, 
\g 

\t agaxátsá ansɛ kiplukpá búlí n'
\b a-g-a-x-á-ts-á an-sɛ ki-p-lu-kp-á bú-lí n-'
\q agax-á-ts-á an-sɛ ki-p-lu-kp-á bú-lí n'
\g 

\t so bunu lɛ bɔɔ́kɔ nɛ́ klɛ,
\b so bu-nu lɛ bɔ-ɔ́-kɔ nɛ́ k-l-ɛ,
\q so bu-nu lɛ bɔ-ɔ́-kɔ nɛ́ kl-ɛ, 
\g 

\t gɛ enu bɔdɔ̃ tulí tɛ́babáɔma mɛ.
\b gɛ e-nu bɔ-dɔ-̃ t-u-lí t-ɛ́-baba-́-ɔ-m-a m-ɛ.
\q gɛ e-nu bɔ-dɔ-̃ tu-lí t-ɛ́-baba-́ɔma m-ɛ. 
\g 

\t Kliso pɛ idzǔ,
\b K-li-so pɛ i-dz-u-̌-,
\q K-li-so pɛ i-dz-ǔ,
\g 

\t mɛ ko,
\b mɛ ko,
\q mɛ ko, 
\g 

\t ɛbakpɛ́ kesí
\b ɛ-ba-kpɛ́ ke-s-í
\q ɛ-ba-kpɛ́ ke-s-í 
\g 

\t kã́ adanyɔ́ ɔma mɛ é,
\b k-ã-́ a-d-any-ɔ́ ɔ-m-a mɛ e-́,
\q k-ã-́ ad-any-ɔ́ ɔma mɛ e-́, 
\g 

\t Begbe butukúi vɛnɔ
\b B-e-gb-e bu-t-u-kú-i v-ɛ-nɔ
\q Be-gb-e bu-tu-kú-i vɛ-nɔ 
\g 

\t Aápɛɛ gaké agɛmɔ ɛ,
