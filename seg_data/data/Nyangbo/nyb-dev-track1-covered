\q bɔ-bat-ɔ-ɖɔ nɛ́ osu-bh-a abh-a á-n-í-i-́, 
\g 

\t bubakóéyi nɛ́ wɔ́ shumɛ,
\b bu-ba-k-ó-é-y-i nɛ́ w-ɔ́ sh-u-mɛ,
\q bu-ba-k-ó-é-yi nɛ́ w-ɔ́ sh-u-mɛ, 
\g 

\t bulíɛ́ gɛ obóɔ,
