\q Ba-dz-ɛ̃-̌ pétéé aba-vɛ kɛ-lɛ́ 
\g 

\t lṹ ɔ,
\b lu-̃-́ ɔ,
\q lu-̃́ ɔ, 
\g 

\t ko bezebekú ɔkɔ́ tolí,
\b ko be-z-e-be-k-ú ɔ-kɔ́ t-o-li-́,
\q ko be-ze-be-k-ú ɔ-kɔ́ to-li-́, 
\g 

\t Gɛ ɛgɛzã soɔ
\b G-ɛ ɛ-g-ɛ-z-ã s-o-ɔ
\q Gɛ ɛgɛz-ã soɔ
\g 

\t Wonɔ epíɛ́
\b W-o-nɔ e-p-íɛ́
\q Wo-nɔ ep-íɛ́ 
\g 

\t bulĩ́ɛ́ mɛ akɔ́ ɔlɔ́.
\b bu-li-̃́-ɛ́ mɛ a-kɔ́ ɔ-lɔ-́-.
\q bu-li-̃́-ɛ́ mɛ a-kɔ́ ɔ-lɔ-́.
\g 

\t woayɛ́ tsyɛ́ kana yedzi?
\b w-o-a-y-ɛ́ tsyɛ́ k-ana y-e-dz-i-?
\q woay-ɛ́ tsyɛ́ k-ana ye-dz-i?
\g 

\t tsyáká nɛ bawá telí,
\b tsy-á-k-á n-ɛ ba-w-á te-li-́,
\q tsy-á-k-á nɛ ba-w-á te-li-́, 
\g 

\t piapíí nɛ́ ɣeɣíyi alɛ mɛ,
\b p-i-a-p-í-í nɛ́ ɣ-e-ɣ-í-y-i alɛ mɛ,
\q piap-í-í nɛ́ ɣeɣ-í-yi alɛ mɛ, 
\g 

\t kɛlɛ akasɛ́...
\b kɛlɛ a-ka-s-ɛ́-.-.-.
\q kɛlɛ a-ka-s-ɛ́-...
\g 

\t osíɛ́ odzii,
\b o-s-íɛ́ o-dz-i-i-,
\q os-íɛ́ o-dz-ii,
\g 

\t babawɛ̃ nɛ samɛ samɛ,
\b baba-w-ɛ̃ n-ɛ s-a-mɛ s-a-mɛ,
\q baba-w-ɛ̃ nɛ sa-mɛ sa-mɛ, 
\g 

\t kɛlɛ salí nɔ́ abagbagba
\b kɛlɛ s-a-lí nɔ́ aba-g-ba-gb-a
\q kɛlɛ sa-lí nɔ́ aba-g-ba-gb-a
\g 

\t bɔkpɛ́ kɛɖaŋu kɔ wɔ akaalɛ
\b bɔ-kpɛ́ kɛ-ɖ-a-ŋ-u kɔ w-ɔ a-ka-alɛ
\q bɔ-kpɛ́ kɛ-ɖaŋu kɔ wɔ a-ka-alɛ 
\g 

\t yoo,
\b y-o-o-,
\q yoo,
\g 

\t batsyiami nɔ́ mɛayɛ́ tenukpó,
\b ba-tsy-i-a-m-i nɔ́ m-ɛ-a-y-ɛ́ te-n-u-kp-o-́,
\q ba-tsy-iami nɔ́ mɛay-ɛ́ te-nu-kp-o-́, 
\g 

\t yoo wɔnɔ gã
\b y-o-o w-ɔ-nɔ g-ã
\q yoo wɔ-nɔ g-ã 
\g 

\t tɛ oti ko bulíɛ́ kú oo.
\b tɛ o-t-i ko bu-l-íɛ́ k-ú o-o-.
\q tɛ oti ko bu-l-íɛ́ k-ú oo.
\g 

\t Wonɔ adzyaalɛ́,
\b W-o-nɔ a-dz-y-a-a-lɛ́,
\q Wo-nɔ a-dz-yaa-lɛ́, 
\g 

\t bezebetsodzi abá
\b be-z-e-be-ts-o-dz-i aba-́
\q be-ze-be-ts-o-dz-i aba-́
\g 

\t so wonɔ gɛ odzi ɛ,
\b so w-o-nɔ gɛ o-dz-i ɛ,
\q so wo-nɔ gɛ o-dz-i ɛ, 
\g 

\t pétéé,
\b pété-e-́,
\q pété-e-́, 
\g 

\t ee tsyámi ɔlɛ klɛ́?
\b e-e tsy-á-m-i ɔ-lɛ k-l-ɛ́-?
\q ee tsy-á-mi ɔ-lɛ kl-ɛ́-?
\g 

\t sɛ obúɔ́ kishu ɔ́ bɔbashɔ̃ nɛ́ klɛ,
\b sɛ o-bú-ɔ́ ki-sh-u ɔ́ bɔba-sh-ɔ-̃ nɛ́ k-l-ɛ,
\q sɛ o-bú-ɔ́ ki-sh-u ɔ́ bɔba-sh-ɔ̃ nɛ́ kl-ɛ, 
\g 

\t ɔlɔkɔ kɛlɔ̃́mɛ́ akɔlɔ,
\b ɔ-lɔkɔ kɛ-lɔ-̃́-m-ɛ́ a-k-ɔ-lɔ-,
\q ɔ-lɔkɔ kɛ-lɔ-̃́-m-ɛ́ akɔ-lɔ-,
\g 

\t gɛ bawa kɛxwɛ̃,
\b gɛ ba-w-a kɛ-x-w-ɛ̃-,
\q gɛ ba-wa kɛ-xw-ɛ̃-,
\g 

\t err agã hohoe telí basɛ kɛgɔ tenikpó kɛsha tenikpó,
\b err a-g-ã h-o-h-o-e te-lí ba-sɛ kɛ-g-ɔ te-n-i-kp-o-́ kɛ-sh-a te-n-i-kp-o-́,
\q err ag-ã hohoe te-lí ba-sɛ kɛ-gɔ te-ni-kp-ó kɛ-sh-a te-ni-kp-o-́, 
\g 

\t aábá anyɛ́núvɔɛ nɔ́ ɔbha,
\b a-á-ba-́ any-ɛ́-nú-v-ɔɛ nɔ́ ɔ-bh-a,
\q a-á-ba-́ any-ɛ́-nú-v-ɔɛ nɔ́ ɔ-bh-a, 
\g 

\t yɛ meklu nɛ́ klɛ,
\b yɛ m-e-k-l-u nɛ́ k-l-ɛ,
\q yɛ meklu nɛ́ kl-ɛ, 
\g 

\t blɔ tsɛrɛ blɔ ɔhɔɛ kpasɛ kibui ɛ́ mɛ,
\b blɔ ts-ɛ-r-ɛ blɔ ɔ-h-ɔɛ kp-a-sɛ ki-bu-i ɛ́ mɛ,
\q blɔ ts-ɛrɛ blɔ ɔh-ɔɛ kp-a-sɛ ki-bu-i ɛ́ mɛ, 
\g 

\t Kɛlɛ ónúpɛ kɛxwɛ̃ nɔ́ nɔ́ gɛ ɛ lɛ,
\b K-ɛ-lɛ ó-nú-pɛ kɛ-x-w-ɛ̃ nɔ́ nɔ́ gɛ ɛ l-ɛ,
\q Kɛ-lɛ ó-nú-pɛ kɛ-xw-ɛ̃ nɔ́ nɔ́ gɛ ɛ l-ɛ, 
\g 

\t babhɛtɛ ɛ-lɛ kɛlɛ bazɛ
\b ba-bhɛtɛ ɛ-lɛ kɛlɛ ba-z-ɛ
\q ba-bhɛtɛ ɛ-lɛ kɛlɛ ba-zɛ
\g 

\t kã́ akakála akála kɔ́ ɔma,
\b k-ã-́ a-ka-k-á-l-a a-k-á-l-a kɔ́ ɔ-m-a,
\q k-ã-́ a-ka-k-á-la ak-á-la kɔ́ ɔm-a, 
\g 

\t Batrugbu ɔma ozhǐtámɛɛ́,
\b B-a-t-r-u-gb-u ɔ-m-a o-z-h-i-̌-t-á-m-ɛ-ɛ́,
\q Batru-gb-u ɔma ozhǐt-á-mɛ-ɛ́, 
\g 

\t má kampɛ nána shú,
\b m-á ka-m-pɛ n-á-n-a sh-u-́,
\q m-á ka-m-pɛ n-á-na sh-u-́, 
\g 

\t Blɔ tsyáká pɛ bɔlɛ kɛlɛ,
\b B-lɔ tsy-á-k-á pɛ bɔ-lɛ kɛ-l-ɛ,
\q B-lɔ tsy-á-k-á pɛ bɔ-lɛ kɛ-l-ɛ, 
\g 

\t err kliso gɛ ɛgɛzã soɔ
\b err k-li-so gɛ ɛ-g-ɛ-z-ã s-o-ɔ
\q err k-li-so gɛ ɛgɛz-ã soɔ
\g 

\t telí nɛ́ klɛ́,
\b te-lí nɛ́ k-lɛ́,
\q te-lí nɛ́ k-lɛ́, 
\g 

\t ebehunɔ,
\b e-be-h-u-nɔ-,
\q e-be-hu-nɔ-,
\g 

\t baba babasɛnɔ ɔlɔ́,
\b ba-b-a baba-s-ɛ-nɔ ɔ-lɔ-́,
\q ba-ba baba-sɛ-nɔ ɔ-lɔ-́, 
\g 

\t Aa err...
\b A-a e-r-r-.-.-.
\q Aa err...
\g 

\t buziwu
\b bu-z-i-w-u
\q bu-ziwu
\g 

