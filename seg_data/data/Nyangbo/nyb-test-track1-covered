\q ba-nɔ nɔ́ ba-da-kpɛ́ bɔ-dɔ-̃ nɛ́ 
\g 

\t kã́ bɔlɔ pétéé kã́ bɔvɛ axwɛ̃ mɛ,
\b k-ã-́ bɔ-lɔ pétéé k-ã-́ bɔ-v-ɛ a-x-w-ɛ̃ mɛ,
\q k-ã-́ bɔ-lɔ pétéé k-ã-́ bɔ-vɛ axw-ɛ̃ mɛ, 
\g 

\t bawá shú,
\b ba-w-á sh-u-́,
\q ba-w-á sh-u-́, 
\g 

\t nɛ́ kɛ́lɛ yɔ̃́ɔ̃́.
\b nɛ́ kɛ-́-lɛ y-ɔ-̃́-ɔ-̃́-.
\q nɛ́ kɛ-́-lɛ yɔ-̃́-ɔ-̃́-.
\g 

\t klɛ balɛ tsyɛ́ dzidze wu avɛ,
\b klɛ ba-lɛ tsyɛ́ dz-i-dz-e w-u a-v-ɛ,
\q klɛ ba-lɛ tsyɛ́ dz-i-dz-e wu av-ɛ, 
\g 

\t kliso kã́ ɔshɔ kpɛ́ ɔmɔ,
\b k-li-so k-ã-́ ɔ-sh-ɔ kpɛ́ ɔ-m-ɔ,
\q k-li-so k-ã-́ ɔ-sh-ɔ kpɛ́ ɔm-ɔ, 
\g 

\t bulĩ́ɛ́ kú oo
\b bu-li-̃́-ɛ́ k-ú o-o
\q bu-li-̃́-ɛ́ k-ú oo
\g 

\t klɛ akpɛ́ ozĩ̌ɛ́
\b klɛ a-kpɛ́ o-z-i-̃-̌-ɛ́
\q klɛ a-kpɛ́ ozĩ̌-ɛ́ 
\g 

\t yɛ baɖɔ sɛ bunuɔ pɛ̃.
\b yɛ ba-ɖɔ sɛ bu-n-u-ɔ p-ɛ̃-.
\q yɛ ba-ɖɔ sɛ bu-nuɔ p-ɛ̃-.
\g 

\t bɔmá kɔ́ banɔ nɔ́,
\b bɔ-m-á kɔ́ ba-nɔ nɔ-́,
\q bɔ-m-á kɔ́ ba-nɔ nɔ-́, 
\g 

\t ɔmɔ kã́ ohe ɔlɛ́ áníí,
\b ɔ-mɔ k-ã-́ o-h-e ɔ-lɛ́ á-n-í-i-́,
\q ɔ-mɔ k-ã-́ ohe ɔ-lɛ́ á-n-í-i-́, 
\g 

\t klɛ ashɛ̃,
\b klɛ a-sh-ɛ̃-,
\q klɛ a-sh-ɛ̃-,
\g 

\t babaákpɛ́ bɔdɔ̃ akɔ́ ɔlɔ́ nɛ́ klɛ́ nɛ́ bulĩ́ɛ́ mɛ,
\b baba-á-kpɛ́ bɔ-dɔ-̃ a-kɔ́ ɔ-l-ɔ́ nɛ́ k-lɛ́ nɛ́ bu-li-̃́-ɛ́ mɛ,
\q baba-á-kpɛ́ bɔ-dɔ-̃ a-kɔ́ ɔl-ɔ́ nɛ́ k-lɛ́ nɛ́ bu-li-̃́-ɛ́ mɛ, 
\g 

\t yɛ ɔkɔ́ mɛzã so yɛ akasɛ́
\b yɛ ɔ-kɔ́ m-ɛ-z-ã so yɛ a-ka-s-ɛ́
\q yɛ ɔ-kɔ́ mɛz-ã so yɛ a-ka-s-ɛ́ 
\g 

\t pétéé,
\b pété-e-́,
\q pété-e-́, 
\g 

\t Kɛlɛ lɛ blɔ batrugbu ɔ pétéé shú,
\b K-ɛ-lɛ lɛ blɔ bat-r-u-gb-u ɔ pétéé sh-u-́,
\q Kɛ-lɛ lɛ blɔ bat-ru-gb-u ɔ pétéé sh-u-́, 
\g 

\t kɛlɛ etíbéle nɛ́ wɔ shumɛ
\b kɛlɛ e-t-í-be-́-l-e nɛ́ w-ɔ sh-u-mɛ
\q kɛlɛ et-í-be-́le nɛ́ wɔ sh-u-mɛ 
\g 

\t yálɛ yenu olumamɔɛ,
\b y-á-lɛ y-e-nu o-lu-m-a-m-ɔ-ɛ,
\q y-á-lɛ ye-nu o-lu-mamɔ-ɛ, 
\g 

\t bɔkpɛ́ ́alɛ́ ko,
\b bɔ-kpɛ́ ́-a-lɛ́ ko,
\q bɔ-kpɛ́ ́a-lɛ́ ko, 
\g 

\t Srṍé nɔ́,
\b S-r-o-̃́-é nɔ-́,
\q Sro-̃́-é nɔ-́, 
\g 

\t ekusíɛ́ akɔ́ gbe,
\b e-ku-s-íɛ́ a-kɔ́ g-be-,
\q e-ku-s-íɛ́ a-kɔ́ g-be-,
\g 

\t ihú Máwú ahɔɛ,
\b i-h-ú M-á-w-ú a-h-ɔ-ɛ,
\q ih-ú M-á-w-ú ahɔ-ɛ, 
\g 

\t oɖotí nɔ́ nɔɔ́lɛ,
\b o-ɖ-o-t-í nɔ́ nɔ-ɔ́-l-ɛ,
\q oɖot-í nɔ́ nɔ-ɔ́-l-ɛ, 
\g 

\t abalɔkɔɛ,
\b aba-lɔ-k-ɔ-ɛ,
\q aba-lɔ-kɔ-ɛ, 
\g 

\t ɔlɛ asɛ blɔ nú
\b ɔ-lɛ a-sɛ blɔ n-ú
\q ɔ-lɛ a-sɛ blɔ n-ú 
\g 

\t pɛ ɔbɔbá alɛ
\b pɛ ɔ-bɔba-́ alɛ
\q pɛ ɔ-bɔba-́ alɛ 
\g 

\t gɛ beklǔ sɛ...
\b gɛ be-k-lu-̌ s-ɛ-.-.-.
\q gɛ be-k-lu-̌ sɛ...
\g 

\t gaké yɛ kɛtsrɛ̌ɔkɔ́ lɔwɛ̃,
\b g-a-ke-́ yɛ kɛ-ts-r-ɛ-̌-ɔ-kɔ́ lɔ-w-ɛ̃-,
\q ga-ke-́ yɛ kɛ-ts-rɛ̌ɔ-kɔ́ lɔ-w-ɛ̃-,
\g 

\t err blɔ pétéé bɔbhɛtɛ kibe abha é
\b err blɔ pétéé bɔ-bhɛtɛ ki-b-e abh-a é
\q err blɔ pétéé bɔ-bhɛtɛ ki-be abh-a é 
\g 

\t (pɔtɛ́) ɛ,
\b (-p-ɔ-t-ɛ́-) ɛ,
\q (pɔt-ɛ́-) ɛ, 
\g 

\t babaplɛ́nɔ olɔ.
\b baba-p-l-ɛ́-nɔ o-lɔ-.
\q baba-pl-ɛ́-nɔ o-lɔ-.
\g 

\t nɛ́ kashɛ̃,
\b nɛ́ ka-sh-ɛ̃-,
\q nɛ́ ka-sh-ɛ̃-,
\g 

\t kliso nyimɔ́ yɛ ogũɔ lɔlɛ,
\b k-li-so ny-i-m-ɔ́ yɛ o-g-u-̃-ɔ lɔ-l-ɛ,
\q k-li-so ny-im-ɔ́ yɛ ogũɔ lɔ-l-ɛ, 
\g 

\t gɛ bɔbhɛtɛ kibúí tinikpó,
\b gɛ bɔ-bhɛtɛ ki-bú-í t-i-n-i-kp-o-́,
\q gɛ bɔ-bhɛtɛ ki-bú-í tini-kp-o-́, 
\g 

\t ésí,
\b é-s-i-́,
\q é-si-́, 
\g 

\t vɛnɔ otugba,
\b v-ɛ-nɔ o-t-u-g-ba-,
\q vɛ-nɔ otug-ba-,
\g 

\t klisoɔ kɛlɛ́ kibe letéɛ́ beti kibúí,
\b k-li-s-o-ɔ kɛ-lɛ́ ki-b-e l-e-té-ɛ́ be-t-i ki-bú-i-́,
\q k-li-soɔ kɛ-lɛ́ ki-be le-té-ɛ́ be-ti ki-bú-i-́, 
\g 

\t ɛklɛ́
\b ɛ-k-lɛ́
\q ɛk-lɛ́ 
\g 

\t so kɛlɛ yɛ lí etété nɛ́ klɛ́,
\b so kɛlɛ yɛ lí e-té-t-é nɛ́ k-lɛ́,
\q so kɛlɛ yɛ lí e-té-t-é nɛ́ k-lɛ́, 
\g 

\t kɛlɛ ohui ɔlɛ́ gɛ odze alɛ,
\b kɛlɛ o-h-u-i ɔ-lɛ́ gɛ o-dz-e a-l-ɛ,
\q kɛlɛ ohui ɔ-lɛ́ gɛ o-dz-e al-ɛ, 
\g 

\t err soɔ kidzǒ lɛ́,
\b err s-o-ɔ ki-dz-o-̌ lɛ́,
\q err soɔ ki-dz-ǒ lɛ́, 
\g 

\t kɛlɛ akasɛ́...
\b kɛlɛ a-ka-s-ɛ́-.-.-.
\q kɛlɛ a-ka-s-ɛ́-...
\g 

\t gɛ enu yálɛ yɛ abakɔ́ sɛ
\b gɛ e-nu y-á-lɛ yɛ aba-kɔ́ sɛ
\q gɛ e-nu y-á-lɛ yɛ aba-kɔ́ sɛ 
\g 

\t anɔ nyá nɔ abhɛtɛ,
\b an-ɔ ny-á nɔ a-bhɛ-t-ɛ,
\q an-ɔ ny-á nɔ a-bhɛ-t-ɛ, 
\g 

\t bɔwá nɔ́.
\b bɔ-w-á n-ɔ́-.
\q bɔ-w-á n-ɔ́-.
\g 

\t mɛ edzě yɛ akasɛ́ mɛ sɛ kana yɛ toró
\b mɛ e-dz-e-̌ yɛ a-ka-s-ɛ́ mɛ sɛ k-ana yɛ t-o-r-o-́
\q mɛ e-dz-ě yɛ a-ka-s-ɛ́ mɛ sɛ k-ana yɛ toró
\g 

\t ɛrr avɔbɔ́ lewu etsiɖi osí nɔ́ abha,
\b ɛ-rr a-v-ɔ-b-ɔ́ l-e-w-u e-ts-i-ɖ-i o-s-í nɔ́ abh-a,
\q ɛ-rr avɔb-ɔ́ lewu e-ts-iɖi os-í nɔ́ abh-a, 
\g 

\t betelí edě err Avɛnɔ.
\b be-te-lí e-de-̌ err A-v-ɛ-nɔ-.
\q be-te-lí e-de-̌ err Avɛ-nɔ-.
\g 

\t bunyí kɛzã gɛ ɔzã nɛ́ blɔ
\b bu-ny-í kɛ-z-ã gɛ ɔ-z-ã nɛ́ blɔ
\q bu-ny-í kɛ-z-ã gɛ ɔz-ã nɛ́ blɔ 
\g 

\t kɛlɛ babalɔkɔ mana esheshe ɔkɔ́,
\b kɛlɛ baba-lɔkɔ m-ana e-sh-e-sh-e ɔ-k-ɔ-́,
\q kɛlɛ baba-lɔkɔ m-ana e-sh-e-sh-e ɔkɔ-́, 
\g 

\t tíkeéwu kɛklagbu ɔ́
\b t-í-ke-é-w-u kɛ-k-l-a-gb-u ɔ́
\q t-í-ke-é-wu kɛ-kla-gb-u ɔ́ 
\g 

\t yoo
\b y-o-o
\q yoo
\g 

\t kifuɔ tsyɔ sɔlɛ tɛ iduku.
\b ki-f-u-ɔ tsy-ɔ s-ɔ-lɛ tɛ i-d-u-ku-.
\q ki-fuɔ tsy-ɔ sɔ-lɛ tɛ idu-ku-.
\g 

\t vũnɔ gagãlĩ gɛ enu budzyuɖí yɛ
\b v-u-̃-nɔ g-a-g-ã-li-̃ gɛ e-nu bu-dz-y-u-ɖ-í yɛ
\q vũ-nɔ gag-ã-li-̃ gɛ e-nu bu-dz-yuɖ-í yɛ 
\g 

\t amɔ́ lɔtu,
\b a-m-ɔ́ lɔ-t-u-,
\q am-ɔ́ lɔ-tu,
\g 

\t Aa nɔ tsyɛ́ nɔ kɔ́ yɔ́.
\b A-a nɔ tsyɛ́ nɔ kɔ́ y-ɔ́-.
\q Aa nɔ tsyɛ́ nɔ kɔ́ y-ɔ́-.
\g 

\t kã́ bubisi alɛ amaneɛ́.
\b k-ã-́ bu-b-i-s-i alɛ a-m-an-e-ɛ́-.
\q k-ã-́ bu-bisi alɛ am-an-e-ɛ́-.
\g 

\t kɔ ba ka,
\b kɔ b-a k-a,
\q kɔ ba k-a, 
\g 

\t ɛzɛmɔ sɛ banɔ ta tá,
\b ɛ-z-ɛ-mɔ sɛ ba-nɔ t-a t-a-́,
\q ɛzɛ-mɔ sɛ ba-nɔ ta ta-́, 
\g 

\t gaké tɛ abaná kibe telí kobulĩ́ɛ́ ŋútɔ́ nɔ́ ko.
\b g-a-ke-́ tɛ aba-n-á ki-b-e te-lí ko-bu-li-̃́-ɛ́ ŋ-ú-t-ɔ́ nɔ́ ko-.
\q ga-ke-́ tɛ aba-n-á ki-be te-lí ko-bu-li-̃́-ɛ́ ŋ-ú-t-ɔ́ nɔ́ ko-.
\g 

\t kivũ.
\b ki-v-u-̃-.
\q ki-vũ.
\g 

\t abhɛtɛ kɔ́ mɛ ansɛ
\b a-bhɛtɛ kɔ́ mɛ an-sɛ
\q a-bhɛtɛ kɔ́ mɛ an-sɛ 
\g 

\t Bɔlɔ ishú ahɔɛ
\b B-ɔ-lɔ i-sh-ú a-h-ɔɛ
\q Bɔ-lɔ i-sh-ú ah-ɔɛ 
\g 

\t tenukpó,
\b te-n-u-kp-o-́,
\q te-nu-kp-o-́, 
\g 

\t ɔsɛ asɛnɛ́,
\b ɔ-sɛ a-s-ɛ-n-ɛ́,
\q ɔ-sɛ asɛn-ɛ́, 
\g 

\t eletéɛ́ nɛ́ okúɔ́ mɛ tsyɛ́ oo,
\b e-l-e-té-ɛ́ nɛ́ o-kú-ɔ́ mɛ tsyɛ́ o-o-,
\q ele-té-ɛ́ nɛ́ o-kú-ɔ́ mɛ tsyɛ́ oo,
\g 

\t ɛ́ alɔkɔ vɛnɔ buvũ nɔ́ mɛ
\b ɛ́ a-lɔkɔ v-ɛ-nɔ bu-v-u-̃ nɔ́ mɛ
\q ɛ́ a-lɔkɔ vɛ-nɔ bu-vũ nɔ́ mɛ 
\g 

\t balɛ lɛ́ pétéé benu bawá gɛ
\b ba-lɛ lɛ́ pétéé be-nu ba-w-á gɛ
\q ba-lɛ lɛ́ pétéé be-nu ba-w-á gɛ 
\g 

\t gɛ enu schnapps tumpá tabha kɛkɔlɔ
\b gɛ e-nu s-c-h-n-a-p-p-s t-u-m-p-á t-abh-a kɛ-k-ɔ-lɔ
\q gɛ e-nu schnapps tump-á t-abh-a kɛ-kɔ-lɔ 
\g 

\t ɛklɛ mɛmɔ sɛ́,
\b ɛ-klɛ m-ɛ-mɔ s-ɛ́,
\q ɛ-klɛ mɛ-mɔ s-ɛ́, 
\g 

\t beti
\b be-t-i
\q be-ti
\g 

\t bɔ lɔkɔ ɔlɛ akɔ́lɔ́ bɔ lɔkɔ tɔ
\b b-ɔ lɔkɔ ɔ-lɛ a-k-ɔ́-l-ɔ́ b-ɔ lɔkɔ t-ɔ
\q bɔ lɔkɔ ɔ-lɛ ak-ɔ́-l-ɔ́ bɔ lɔkɔ tɔ
\g 

\t pétéé bɔbatɔɖɔ ɛlɛ́,
\b pétéé bɔ-bat-ɔ-ɖɔ ɛ-lɛ́,
\q pétéé bɔ-bat-ɔ-ɖɔ ɛ-lɛ́, 
\g 

\t beti
\b be-t-i
\q be-ti
\g 

\t abadalɛ̃,
\b aba-d-a-l-ɛ̃-,
\q aba-dal-ɛ̃-,
\g 

\t ee kifúɔ́ akɔ́ sɛ...
\b e-e ki-f-ú-ɔ́ a-kɔ́ s-ɛ-.-.-.
\q ee ki-f-ú-ɔ́ a-kɔ́ sɛ...
\g 

\t gaké tɛ abaná kibe telí kobulĩ́ɛ́ ŋútɔ́ nɔ́ ko.
\b g-a-ke-́ tɛ aba-n-á ki-b-e te-lí ko-bu-li-̃́-ɛ́ ŋ-ú-t-ɔ́ nɔ́ ko-.
\q ga-ke-́ tɛ aba-n-á ki-be te-lí ko-bu-li-̃́-ɛ́ ŋ-ú-t-ɔ́ nɔ́ ko-.
\g 

\t ebolé,
\b e-b-o-l-e-́,
\q ebole-́, 
\g 

\t pɛ bakpɛ́ɛ nɛ́ buvũɔ mɛ,
\b pɛ ba-kp-ɛ́-ɛ nɛ́ bu-v-u-̃-ɔ mɛ,
\q pɛ ba-kp-ɛ́-ɛ nɛ́ bu-vũɔ mɛ, 
\g 

\t gɛ enu bɔdɔ̃ tulí tɛ́babáɔma mɛ.
\b gɛ e-nu bɔ-dɔ-̃ t-u-lí t-ɛ́-baba-́-ɔ-m-a m-ɛ.
\q gɛ e-nu bɔ-dɔ-̃ tu-lí t-ɛ́-baba-́ɔma m-ɛ. 
\g 
